{
  "name": "mishisqlsupport",
  "displayName": "MishiSqlSupport",
  "description": "MishiSQL Support for Vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlosHernandez14/mishisql_extension_support.git"
  },
  "license": "MIT",
  "publisher": "CarlosHernandez14",
  "contributes": {
    "languages": [{
      "id": "mishisql",
      "aliases": ["mishisql", "mishisql"],
      "extensions": [".mishisql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mishisql",
      "scopeName": "source.mishisql",
      "path": "./syntaxes/mishisql.tmLanguage.json"
    }]
  }
}
